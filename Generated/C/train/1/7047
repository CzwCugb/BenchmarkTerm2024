typedef enum {false, true} bool;
extern int __VERIFIER_nondet_int(void);

int main() {
  int x = __VERIFIER_nondet_int();
  int y = __VERIFIER_nondet_int();
  
  while(x != x) {
    while(x >= y) {
      x = 13;
      y += 3;
  }
    for(int i=0;i<7;i++) {
      if(6 < x) {
        x -= 1;
    }
  }
  }
  
  
  
}
